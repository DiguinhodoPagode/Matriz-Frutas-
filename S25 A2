Frutas=[[ "MaÃ§a", "Banana"]], [["Laranja", "Uva"]]
def adicionar_fruta(cesta,Fruta):
     cesta.append(Fruta)
def romover_fruta (cesta,Fruta):
        for linha in cesta :
            if fruta in linha:
                linha.remove(Fruta)                                                
                break
adicionar_fruta(frutas(0), "manga")
print(frutas)
remover_fruta(frutas "Bananas")
print(frutas)
